import Foundation

func solution(_ q:Int, _ r:Int, _ code:String) -> String {
    var arr: [String] = code.map{String($0)}
    var result: [String] = []
    for (index, value) in arr.enumerated() {
        var num = index % q
        if num == r {
            result.append(value)
        }
    }
    
    return result.joined()
}
